<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.gere0018.rescueApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <icon density="mdpi" src="www/media/icons/icon-48px.png" />
    <icon density="hdpi" src="www/media/icons/icon-72px.png" />
    <icon density="xhdpi" src="www/media/icons/icon-96px.png" />
    <icon density="xxhdpi" src="www/media/icons/icon-144px.png" />
    <icon density="xxhdpi" src="www/media/icons/icon-192px.png" />
    <name>gere0018-Rescue</name>
    <description>
        An emergency application that allows users to call their emergency contacts in case of danger.
    </description>
    <author email="gere0018@algonquinlive.com" href="https://github.com/gere0018/rescue">
       Nehmat Gereige
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="android-minSdkVersion" value="18" />
</widget>
